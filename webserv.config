server {
	listen 127.0.0.1:7700;
	server_name example.com;
	client_max_body_size 1000;
	root /website/;
	index index.html;

	location / {
		allow_methods GET;
	}
}

server {
	listen 127.0.0.1:7701;
	server_name example.at;
	client_max_body_size 1000;
	root /website/;
	index index.html;

	location / {
		allow_methods GET;
	}
}