server {
	listen 7700;
	server_name localhost;
	client_max_body_size 10000;
	root /website/;
	index index.html;

	location / {
		allow_methods GET;
		proxy_pass /put_save;
		autoindex off;
		allowed_cgi_extension .blah;
		client_max_body_size 100;
	}

	location /put_test {
		allow_methods PUT;
	}

	location /directory/ {
		allow_methods GET;
		index youpi.bad_extension;
		root YoupiBanane;
	}

	location *.bla {
		allow_methods POST;
		cgi_script ubuntu_cgi_tester;
	}

	location /pages {
		allow_methods GET;
		autoindex on;
	}
		location /cgi-bin {
		allow_methods GET POST;
		client_max_body_size 100;
		allowed_cgi_extension .blah .py;
	}
	error_page 400 /pages/400.html;
	error_page 404 /pages/404.html;
	error_page 405 /pages/405.html;
	error_page 500 /pages/500.html;
	error_page 502 /pages/502.html;
	error_page 503 /pages/503.html;
	error_page 505 /pages/505.html;
	rewrite /test /string;
}
